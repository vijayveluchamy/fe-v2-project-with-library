{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "allowJs": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "rootDir": "./",
      "outDir": "./dist",
      "baseUrl": "./",
      "typeRoots": [
        "./node_modules/@types",
        "./node_modules/@sapui5/types"
      ],
      "paths": {
          "com/myorg/library1/*": [
              "./src/com/myorg/library1/*"
          ]
      }
  },
  "include": [
      "./src/**/*",
      "./test/**/*"
  ]
}